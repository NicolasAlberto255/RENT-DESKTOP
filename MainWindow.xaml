<Window x:Class="RENT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RENT"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="800">
    <Grid>
        <Frame x:Name="Main" Margin="0,40,0,23" Width="800" NavigationUIVisibility="Hidden"/>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Height="35" Grid.ColumnSpan="2">
            <Button x:Name="HomeBtn" Content="Inicio" Margin="10,5,0,0" Click="inicioBtn_Click"/>
            <Button x:Name="rgUsuarioBtn" Content="Registrar Usuario" Margin="10,5,0,0" Click="rgUsuarioBtn_Click"/>
            <Button x:Name="bUsuariosBtn" Content="Buscar Usuarios" Margin="10,5,0,0" Click="bUsuariosBtn_Click"/>
            <Button x:Name="addReservaBtn" Content="Añadir Reserva" Margin="10,5,0,0" Click="addReservaBtn_Click"/>
            <Button x:Name="addDepartamentoBtn" Content="Añadir Departamento" Margin="10,5,0,0" Click="addDepartamentoBtn_Click"/>
        </StackPanel>
        <Image x:Name="logoImg" Margin="158,156,158,156" Source="C:\Users\Lab02\Desktop\RENT-DESKTOP\img\6T0bcLp.png" Stretch="Fill"/>
    </Grid>
</Window>
